<templateSet group="Java">
  <template name="syso" value="System.out.$END$" description="System.out" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="wnclogger" value="private static final org.apache.logging.log4j.Logger LOGGER = wt.log4j.LogR.getLogger($NAME$.class.getName());" description="Create a windchill logger" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
</templateSet>